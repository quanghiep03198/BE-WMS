{
	"name": "node_server",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start -b swc --type-check --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"prepare": "husky",
		"typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
		"migration:show": "npm run typeorm migration:show -- -d ./src/databases/data-source.ts",
		"migration:create": "npm run typeorm -- migration:create ./src/databases/migrations/%npm_config_name%",
		"migration:run": "npm run typeorm migration:run -- -d src/databases/data-source.ts",
		"migration:revert": "npm run typeorm migration:revert -- -d ./src/databases/data-source.ts",
		"seeder:run": "npm run typeorm seed:run -- -d ./src/databases/data-source.ts"
	},
	"dependencies": {
		"@faker-js/faker": "^9.0.1",
		"@nestjs/axios": "^3.1.0",
		"@nestjs/cache-manager": "^2.2.2",
		"@nestjs/common": "^10.0.0",
		"@nestjs/config": "^3.2.3",
		"@nestjs/core": "^10.0.0",
		"@nestjs/event-emitter": "^2.1.1",
		"@nestjs/jwt": "^10.2.0",
		"@nestjs/mapped-types": "*",
		"@nestjs/passport": "^10.0.3",
		"@nestjs/platform-express": "^10.0.0",
		"@nestjs/throttler": "^6.2.1",
		"@nestjs/typeorm": "^10.0.2",
		"axios": "^1.7.7",
		"bcrypt": "^5.1.1",
		"cache-manager": "^5.7.6",
		"cache-manager-redis-store": "^2.0.0",
		"compression": "^1.7.4",
		"date-fns": "^3.6.0",
		"dotenv": "^16.4.5",
		"helmet": "^7.1.0",
		"ioredis": "^5.4.1",
		"lodash": "^4.17.21",
		"lz-string": "^1.5.0",
		"morgan": "^1.10.0",
		"mssql": "^10.0.4",
		"nestjs-i18n": "^10.4.5",
		"passport": "^0.7.0",
		"passport-jwt": "^4.0.1",
		"passport-local": "^1.0.0",
		"redis": "^4.7.0",
		"reflect-metadata": "^0.2.0",
		"rxjs": "^7.8.1",
		"typeorm": "^0.3.20",
		"typeorm-extension": "^3.6.1",
		"util": "^0.12.5",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@automock/adapters.nestjs": "^2.1.0",
		"@automock/jest": "^2.1.0",
		"@commitlint/cli": "^19.4.1",
		"@commitlint/config-conventional": "^19.4.1",
		"@nestjs/cli": "^10.0.0",
		"@nestjs/schematics": "^10.0.0",
		"@nestjs/testing": "^10.0.0",
		"@swc/cli": "^0.4.0",
		"@swc/core": "^1.7.23",
		"@swc/jest": "^0.2.36",
		"@types/bcrypt": "^5.0.2",
		"@types/express": "^4.17.17",
		"@types/jest": "^29.5.2",
		"@types/lodash": "^4.17.9",
		"@types/morgan": "^1.9.9",
		"@types/node": "^20.3.1",
		"@types/supertest": "^6.0.0",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"copy-webpack-plugin": "^12.0.2",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-unused-imports": "^4.1.3",
		"husky": "^9.1.5",
		"jest": "^29.7.0",
		"lint-staged": "^15.2.10",
		"prettier": "^3.0.0",
		"prettier-plugin-organize-imports": "^4.0.0",
		"run-script-webpack-plugin": "^0.2.0",
		"source-map-support": "^0.5.21",
		"supertest": "^7.0.0",
		"swc-loader": "^0.2.6",
		"ts-jest": "^29.1.0",
		"ts-loader": "^9.4.3",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0",
		"tsconfig-paths-webpack-plugin": "^4.1.0",
		"typescript": "^5.1.3",
		"webpack": "^5.94.0",
		"webpack-cli": "^5.1.4",
		"webpack-node-externals": "^3.0.0"
	},
	"jest": {
		"transform": {
			"^.+\\.(t|j)s?$": [
				"@swc/jest"
			]
		}
	},
	"lint-staged": {
		"*.ts": [
			"npm run lint",
			"npm run format",
			"git add ."
		]
	}
}
